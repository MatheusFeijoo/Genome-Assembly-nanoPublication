    dcterms:title "Extensive flagellar remodeling during the complex life cycle of Paratrypanosoma, an early-branching trypanosomatid"@en ;
    prism:keywords "cytostome"@en, "evolution"@en, "flagellar remodeling"@en, "haptomonads"@en, "trypanosomatid"@en ;
    cito:extends sub:citingOrganism1 ;
    data:hasDescription sub:abstract .

  sub:paper_pmid a data:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/29078369/> ;
