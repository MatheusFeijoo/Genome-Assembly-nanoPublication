    dcterms:title "Whole genome sequencing of multiple Leishmania donovani clinical isolates provides insights into population structure and mechanisms of drug resistance"@en ;
    cito:extends sub:citingOrganism1 ;
    data:hasDescription sub:abstract .

  sub:paper_pmid a data:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/22038251/> ;
