    dcterms:title "The DNA sequence of chromosome I of an African trypanosome: gene content, chromosome organisation, recombination and polymorphism"@en ;
    cito:extends sub:citingOrganism1 ;
    data:hasDescription sub:abstract .

  sub:paper_pmid a data:PrimaryResourceIdentifier ;
    prov:value <https://pubmed.ncbi.nlm.nih.gov/12907729/> ;
