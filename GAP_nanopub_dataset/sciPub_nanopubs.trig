  <https://doi.org/10.1038/ng2053> a fabio:journalArticle ;
    data:hasIdentifier sub:14_paper_doi , sub:14_paper_pmid , sub:14_paper_pmcid ;
    dc:language "eng" ;
    dc:title "Comparative genomic analysis of three Leishmania species that cause diverse human disease"@en ;
    data:hasDescription sub:14_abstract ;
    cito:extends sub:citingOrganism1 ;
