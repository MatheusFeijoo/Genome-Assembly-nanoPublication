@prefix this: <http://example.org/nanopub-validator-example/RAtAU6U_xKTH016Eoiu11SswQkBu1elB_3_BoDJWH3arA> .
@prefix sub: <http://example.org/nanopub-validator-example/RAtAU6U_xKTH016Eoiu11SswQkBu1elB_3_BoDJWH3arA#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix pav: <http://purl.org/pav/> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix npa: <http://www.nanopub.org/nschema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix ncbi_asbID: <https://www.ncbi.nlm.nih.gov/assembly/> .
@prefix edam: <http://edamontology.org/page> .
@prefix ncbi_taxID: <https://www.ncbi.nlm.nih.gov/Taxonomy> .
@prefix so: <http://purl.obolibrary.org/obo/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix fbcv: <http://purl.obolibrary.org/obo/> .
@prefix pato: <http://purl.obolibrary.org/obo/> .
@prefix obi: <http://purl.obolibrary.org/obo/> .
@prefix eco: <https://evidenceontology.org/term/> .
@prefix efo:  <http://www.ebi.ac.uk/efo/> .




sub:Head {
  this: np:hasAssertion sub:assertion ;
    np:hasProvenance sub:provenance ;
    np:hasPublicationInfo sub:pubinfo ;
    a np:Nanopublication .
}

## representing a strain and its assembly.
sub:assertion {  
  ncbi_asbID:XXXXXXX rdfs:type edam:data_2292 , edam:data_0925 ; ## asbID type Genbank Accession ID and type sequence assembly.
    sio:SIO_000628 sub:strain ; ## refers to sub:strain.
    prov:wasGeneratedBy sub:gbAssembly ; ## generated by an assembly.
    prov:wasAssociatedWith sub:refseq ; ## associated with a refseq.
    prov:wasGeneratedBy sub:wgs ; ## generated by Whole Genome Sequence.
    edam:data_3273 <biosampleID> ; ## biosample ID.
    ncit:C175890 <bioprojectID> ; ## bioproject ID.    
    ncit:C53471 "clone 1.1"@en . ## isolate type (example clone 1.1).

  sub:strain sio:SIO_000628 sio:SIO_010055 ; ## refers to a biological strain.
    edam:data_1046 "infraspecific name"@en ; ## strain name.
    sio:SIO_000497 <id_nanopub_organism> . ## version of organism.

  <id_nanopub_organism> sio:SIO_000628 sio:SIO_010000 . ## id refers to organism.

  sub:gbAssembly sio:SIO_000628 so:SO_0001248 ; ## sub:assembly refers to assembly.
    rdfs:type fbcv:FBcv_0003238 ; ## type genome representation - partial (can be fbcv:FBcv_0003237 (genome representation - full)).**
    rdfs:type pato:PATO_0001375 ; ## type haploid (example, needs to insert the options).**
    ## Can be: Assembly; pato:PATO_0001375 (haploid); haploid-with-alt-loci;**
    ## pato:PATO_0001394 (diploid); Unresolved Diploid; Linked Pseudihaplotype Assemblies.**
    ncit:C25554 so:SO_0000340 ; ## assembly level chromosome (example, needs to insert the options)
    ## Can be: ero:ERO_0002016 (complete genome); so:SO_0000340 (chromosome);**
    ## so:SO_0000148 (scaffold); so:SO_0000149 (contig).**
    ncit:C71460 "method"@en ; ## assembly method.
    obi:OBI_0001939 "genome coverage"@en ; ## genome coverage.
    efo:EFO_0003739 "sequencer technology"@en ; ## sequence techonology.
    rdfs:type pav:latest_version .

  sub:refseq sio:SIO_000628 ncit:C45335 ; ## refseq refers to refseq.
    rdfs:type geno:GENO_0000914 ; ## refseq type reference genome (can be ncit:C67045 (representative)).**
    edam:data_1098 "RefSeq Assembly Accession" . ## refseq assembly accession.

  sub:gbAssembly prov:wasAssociatedWith sub:refseq . 

  sub:wgs sio:SIO_000628 ncit:C101294 ;
    dcterms:identifier "wgs project identifier" ;
    pav:version "version" .
}

sub:provenance {
  sub:assertion a prov:Entity ; ## assertion type entity
    prov:wasGeneratedBy sub:automaticAssertion ; ## assertion was generated by automatic assertion.
    prov:wasAttributedTo sub:submitter ; ## atribution of the assertion.
    pav:curatedBy sub:submitter ; ## assertion curated by submitter.
    prov:hadPrimarySource <https://www.ncbi.nlm.nih.gov/assembly/>; ## primary source of the assertion.
    ncit:C101282 sub:comment ; ## has description.
    dcterms:accessRights <https://www.ncbi.nlm.nih.gov/home/about/policies/> ; ## access rights.
    eco:ECO_0000501 <https://www.ncbi.nlm.nih.gov/assembly/GCA_003594625.1/> ; ## evidence of the automatic assertion.
    prov:wasDerivedFrom sub:ftp ;
    sio:SIO_000253 <DOI> .

  sub:submitter a foaf:organization, prov:Agent ;
    foaf:name "name"@en .

  sub:automaticAssertion a prov:Activity;
    rdfs:type eco:ECO_0000203; ## type automatic assertion.
    prov:wasAssociatedWith ncit:C45799, sub:submitter; ## associated with NCBI.
    dcterms:dateSubmitted "DATETIME"^^xsd:datetime; ## don't have date of the publication, using dateSubmitted and the date when collected.
    npa:creationDay "number"^^xsd:integer ; ## like 2020-01-21.
    npa:creationMonth "number"^^xsd:integer ; ## like 2020-01.
    npa:creationYear "number"^^xsd:integer . ## like 2020.   

  sub:comment a prov:Entity ;
    rdfs:type dcterms:description ;
    prov:value "comment"@en ;
    prov:wasQuotedFrom <https://www.ncbi.nlm.nih.gov/assembly/GCA_003594625.1/> .
    
  sub:ftp a prov:Entity ;
    rdfs:type dcterms:dataset ;
    pav:curatedBy sub:submitter ;
    prov:hadPrimarySource <https://ftp.ncbi.nlm.nih.gov/genomes/all/GCA/015/455/285/GCA_015455285.1_SC43_diploid_1.0/>.

}


sub:publicationInfo {
  this: a prov:entity; ## nanopub type entity.
    prov:wasGeneratedBy sub:automaticAssertion ; ## generated by automatic assertion
    prov:wasAttributedTo sub:software , sub:npubCreators ; ## attributed by software agent and curators
    prov:wasDerivedFrom ncit:C45799; ## derived from NCBI.
    prov:hadPrimarySource <https://www.ncbi.nlm.nih.gov/assembly/> ; ## primary resources of the assertion.**
    dcterms:accessRights <http://opendatacommons.org/licenses/odbl/1.0/> ; ## see the correct accessRights.**
    dcterms:subject so:SO_0001248 , sio:SIO_010055. ## subject strain and assembly.

  sub:automaticAssertion a prov:Activity ; ## automatic assertion type activity.
    rdfs:type eco:ECO_0000203; ## automatic assertion
    prov:wasAssociatedWith ncit:C45799, prov:softwareAgent, ncit:C122473; ## associated with NCBI, software agent and curators
    dcterms:dateSubmitted "DATETIME"^^xsd:datetime; ## don't have date of the publication, using dateSubmitted and the date when collected
    npa:creationDay "number"^^xsd:integer ; ## like 2020-01-21
    npa:creationMonth "number"^^xsd:integer ; ## like 2020-01
    npa:creationYear "number"^^xsd:integer . ## like 2020 

  ncit:C45799 a foaf:organization, prov:Agent ; ## NCBI type organization/agent
    foaf:name "NCBI - National Center for Biotechnology Information"@en .
    
  sub:software a prov:Agent; ## software type agent.
    rdfs:type prov:softwareAgent; 
    foaf:name "APPNAME"@en;
    pav:version "v1.1";
    pav:createdBy <orcid1>, <orcid2>, <orcid3>, <...>; ## created by orcid.
    dcterms:source <githubLink> ; ## link to github.
    edam:data_1188 <DOI> . ## doi URL.

  sub:npubCreators a prov:Agent; ## curators type agents.
    rdfs:type ncit:C122473 ;
    pav:createdBy <orcid1>, <orcid2>, <orcid3>, <orcid4> . ##created by orcid.
}