@prefix this: <http://example.org/nanopub-validator-example/RAtAU6U_xKTH016Eoiu11SswQkBu1elB_3_BoDJWH3arA> .
@prefix sub: <http://example.org/nanopub-validator-example/RAtAU6U_xKTH016Eoiu11SswQkBu1elB_3_BoDJWH3arA#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix pav: <http://purl.org/pav/> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix rdf: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix ncbi_asbID: <https://www.ncbi.nlm.nih.gov/assembly/> .
@prefix biol: <https://ontologi.es/biol/ns.html> .
@prefix edam: <http://edamontology.org/page> .
@prefix ncbi_taxID: <https://www.ncbi.nlm.nih.gov/Taxonomy> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix data: <http://purl.org/spar/datacite> .
@prefix prism: <http://prismstandard.org/namespaces/basic/2.0/> .
@prefix doi: <http://dx.doi.org/> .
@prefix wiki: <https://www.wikidata.org/wiki/Property:> .
@prefix cito: <http://purl.org/spar/cito> .



sub:Head {
  this: np:hasAssertion sub:assertion ;
    np:hasProvenance sub:provenance ;
    np:hasPublicationInfo sub:pubinfo ;
    a np:Nanopublication .
}

## representing a strain and its assembly.
sub:assertion {  
  doi:ID a fabio:journalArticle ;
    data:hasIdentifier sub:paper_doi , sub:paper_pmid , sub:paper_pmcid ;
    foaf:topic_interest "publication type"@en ; ## example: comparative study.
    dc:language "eng" ;
    dc:title "title"@en ;
    data:hasDescription sub:abstract ;
    prism:keywords "keyword 1" , "keyword 2" , "keyword 3" ;
    cito:extends sub:citingOrganism1.

  sub:paper_doi a data:PrimaryResourceIdentifier ;
    prov:value "id doi" ;
    data:usesIdentifierScheme data:doi .
  
  sub:paper_pmid a data:PrimaryResourceIdentifier ;
    prov:value "id pmid" ;
    data:usesIdentifierScheme edam:data_1187 . ## uses identifier scheme pmid

  sub:paper_pmcid a data:PrimaryResourceIdentifier ;
    prov:value "id pmcid" ;
    data:usesIdentifierScheme wiki:P932 . ## uses identifier scheme pmcid

  sub:abstract data:hasDescriptionType data:abstract ;
    prov:value "abstract"@en .

  sub:citingOrganism1 a cito:citation;
    cito:hasCitingEntity doi:ID;
    cito:hasCitationCharacterization cito:extends;
    cito:hasCitingEntity ncbi_asbID:XXXXXXX;
    data:hasIdentifier sub:organism1.
  
  sub:organism1 a data:PrimaryResourceIdentifier ;
    prov:value "id nanopub" ;
    data:usesIdentifierScheme "nanopub" . ## uses identifier scheme pmcid


}

sub:provenance {
  sub:assertion a prov:Entity ; ## assertion type entity
    prov:wasGeneratedBy sub:journalPublication ; ## assertion was generated by automatic assertion.
    prov:wasAttributedTo sub:authors ; ## atribution of the assertion.
    prov:hadPrimarySource sub:paper_doi; ## primary source of the assertion.
    dc:accessRights <https://www.ncbi.nlm.nih.gov/home/about/policies/> ; ## access rights.
    eco:ECO_0000501 <https://pubmed.ncbi.nlm.nih.gov/29708484/> ; ## evidence of the automatic assertion.
    prov:wasDerivedFrom sub:journal .

  sub:authors a prov:Agent ;
    foaf:name "authors"@en .

  sub:journalPublication a prov:Activity ;
    rdf:type eco:ECO_0006016 ; ## type published clinical study evidence.
    pav:provideBy sub:journal ;
    dc:dateSubmitted "DATETIME"^^xsd:datetime ; ## don't have date of the publication, using dateSubmitted and the date when collected.
    npa:creationDay "number"^^xsd:integer ; ## like 2020-01-21.
    npa:creationMonth "number"^^xsd:integer ; ## like 2020-01.
    npa:creationYear "number"^^xsd:integer . ## like 2020.   

  sub:paper_doi a data:PrimaryResourceIdentifier ;
    prov:value "id doi" ;
    data:usesIdentifierScheme data:doi .  
    
  sub:journal a prov:Agent ;
    rdf:type fabio:Journal ;
    dc:title "jornal name"@en ;
    prism:volume "10"^^xsd:integer ;
    prov:hadPrimarySource <https://www.microbiologyresearch.org/content/journal/mgen>.

}


sub:publicationInfo {
  this: a prov:entity; ## nanopub type entity.
    prov:wasGeneratedBy sub:automaticAssertion ; ## generated by automatic assertion
    prov:wasAttributedTo sub:software , sub:npubCreators ; ## attributed by software agent and curators
    prov:wasDerivedFrom fabio:Journal; ## derived from journal.
    prov:hadPrimarySource <https:///pubmed.ncbi.nlm.nih.gov/> ; ## primary resources of the assertion.**
    dc:accessRights <http://opendatacommons.org/licenses/odbl/1.0/> ; ## see the correct accessRights.**
    dc:subject obo:SO_0001248 , sio:SIO_010055 , fabio:journalArticle. ## subject strain, assembly and journal article.

  sub:automaticAssertion a prov:Activity ; ## automatic assertion type activity.
    rdf:type eco:ECO_0000203; ## automatic assertion
    prov:wasAssociatedWith ncit:C45799, prov:softwareAgent, sub:npubCreators , ncit:C42881; ## associated with NCBI, software agent, curators and pubmed.
    dc:dateSubmitted "DATETIME"^^xsd:datetime; ## don't have date of the publication, using dateSubmitted and the date when collected
    npa:creationDay "number"^^xsd:integer ; ## like 2020-01-21
    npa:creationMonth "number"^^xsd:integer ; ## like 2020-01
    npa:creationYear "number"^^xsd:integer . ## like 2020 

  ncit:C45799 a foaf:organization, prov:Agent ; ## NCBI type organization/agent
    foaf:name "NCBI - National Center for Biotechnology Information"@en .
    
  sub:software a prov:Agent; ## software type agent.
    rdf:type prov:softwareAgent; 
    foaf:name "APPNAME"@en;
    pav:version "v1.1";
    pav:createdBy <orcid1>, <orcid2>, <orcid3>, <...>; ## created by orcid.
    dc:source <githubLink> ; ## link to github.
    edam:data_1188 <DOI> . ## doi URL.

  sub:npubCreators a prov:Agent; ## curators type agents.
    rdf:type ncit:C122473 ;
    pav:createdBy <orcid1>, <orcid2>, <orcid3>, <orcid4> . ##created by orcid.

  ncit:C42881 a foaf:organization, prov:Agent ; ## NCBI type organization/agent
    foaf:name "PubMed"@en .  
}