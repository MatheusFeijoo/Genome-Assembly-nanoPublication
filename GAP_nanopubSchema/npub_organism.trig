@prefix this: <http://example.org/nanopub-validator-example/RAtAU6U_xKTH016Eoiu11SswQkBu1elB_3_BoDJWH3arA> .
@prefix sub: <http://example.org/nanopub-validator-example/RAtAU6U_xKTH016Eoiu11SswQkBu1elB_3_BoDJWH3arA#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix pav: <http://purl.org/pav/> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix npa: <http://www.nanopub.org/nschema#> .
@prefix rdf: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix ncbi_asb: <https://www.ncbi.nlm.nih.gov/assembly/> .
@prefix edam: <http://edamontology.org/page> .
@prefix ncbi_taxID: <https://www.ncbi.nlm.nih.gov/Taxonomy> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix ncit: <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix eco: <https://evidenceontology.org/term/> .


sub:Head {
  this: np:hasAssertion sub:assertion ;
    np:hasProvenance sub:provenance ;
    np:hasPublicationInfo sub:pubinfo ;
    a np:Nanopublication .
}

## representing a organism.
sub:assertion {
  ncbi_taxID:XXXXXXX sio:SIO_000628 sio:SIO_010000 ; ## taxID refers to organism.
      foaf:name "organism name"@en ; ## name of the organism.
      rdf:type obo:NCBITaxon_5653 ; ## obo:NCBITaxon_5653 / kinetoplastids (needs to be reviewed).**
      ncit:C101282 edam:data_0925 ; ## has sequence assembly.
      ncit:c67045 <http://url.representative.genome> . ## representative url of the representative genome (needs to be reviewed).**
  edam:data_0925 ncit:c25256 "34"^^xsd:integer . ## quantity of genome assemblies.
}

sub:provenance {
  sub:assertion a prov:Entity; ## assertion type entity
    prov:wasGeneratedBy sub:automaticAssertion ; ## assertion was generated by automatic assertion.
    prov:wasAttributedTo ncit:C45799 ; ## atribution of the assertion to NCBI.
    pav:curatedBy ncit:C45799; ## assertion curated by NCBI.
    prov:hadPrimarySource <https://www.ncbi.nlm.nih.gov/Taxonomy>, <https://www.ncbi.nlm.nih.gov/genome/?term=txid> ; ## primary resources of the assertion.**
    ncit:C101282 sub:description ; ## has description.
    ncit:C101282 sub:citation ; ## has bibliographic citation.
    dc:accessRights <https://www.ncbi.nlm.nih.gov/home/about/policies/> ; ## access rights.
    eco:ECO_0000501 <https://www.ncbi.nlm.nih.gov/Taxonomy/Browser/wwwtax.cgi?mode=Info&id=5693> . ## evidence of the automatic assertion.
  
  sub:automaticAssertion a prov:Activity ; ## automatic assertion type activity.
    rdf:type eco:ECO_0000203; ## automatic assertion.
    prov:wasAssociatedWith ncit:C45799; ## associated with NCBI.
    dc:dateSubmitted "DATETIME"^^xsd:datetime; ## don't have date of the publication, using dateSubmitted and the date when collected.
    np:creationDay "18"^^xsd:integer ; ## like 2020-01-21.
    np:creationMonth "10"^^xsd:integer ; ## like 2020-01.
    np:creationYear "2020"^^xsd:integer . ## like 2020. 

  ncit:C45799 a foaf:organization, prov:Agent ; ## NCBI type organization/agent.
    foaf:name "NCBI - National Center for Biotechnology Information"@en ;
    dc:source <URL> . 

  sub:description a prov:Entity; ## description type entity.
    rdf:type dc:description;
    prov:value "description"@en; 
    prov:wasQuotedFrom <https://www.ncbi.nlm.nih.gov/genome/?term=txid> . ## quoted from url.

  sub:citation a prov:Entity; ## bibliographic citation type entity.
    rdf:type dc:bibliographicCitation;
    prov:value "Schoch CL, et al. NCBI Taxonomy: a comprehensive update on curation, resources and tools. Database (Oxford). 2020: baaa062."@en ;
    dc:source <PMID> ;
    dc:source <https://doi.org/10.1093/database/baaa062> . ## source url.
}


sub:publicationInfo {
  this: a prov:entity; ## nanopub type entity.
    prov:wasGeneratedBy sub:automaticAssertion ; ## generated by automatic assertion
    prov:wasAttributedTo sub:software , sub:npubCreators ; ## attributed by software agent and curators
    prov:wasDerivedFrom ncit:C45799; ## derived from NCBI.
    prov:hadPrimarySource <https://www.ncbi.nlm.nih.gov/Taxonomy>, <https://www.ncbi.nlm.nih.gov/genome/?term=txid> ; ## primary resources of the assertion.**
    dc:accessRights <http://opendatacommons.org/licenses/odbl/1.0/> ; ## see the correct accessRights.**
    dc:subject sio:SIO_010000. ## subject organism.

  sub:automaticAssertion a prov:Activity ; ## automatic assertion type activity.
    rdf:type eco:ECO_0000203; ## automatic assertion
    prov:wasAssociatedWith ncit:C45799, prov:softwareAgent, ncit:C122473; ## associated with NCBI, software agent and curators
    dc:dateSubmitted "DATETIME"^^xsd:datetime; ## don't have date of the publication, using dateSubmitted and the date when collected
    np:creationDay "18"^^xsd:integer ; ## like 2020-01-21
    np:creationMonth "10"^^xsd:integer ; ## like 2020-01
    np:creationYear "2020"^^xsd:integer . ## like 2020 

  ncit:C45799 a foaf:organization, prov:Agent ; ## NCBI type organization/agent
    foaf:name "NCBI - National Center for Biotechnology Information"@en .
    
  sub:software a prov:Agent; ## software type agent.
    rdf:type prov:softwareAgent; 
    foaf:name "APPNAME"@en;
    pav:version "v1.1";
    pav:createdBy <orcid1>, <orcid2>, <orcid3>, <...>; ## created by orcid.
    dc:source <githubLink> ; ## link to github.
    edam:data_1188 <DOI> . ## doi URL.

  sub:npubCreators a prov:Agent; ## curators type agents.
    rdf:type ncit:C122473 ;
    pav:createdBy <orcid1>, <orcid2>, <orcid3>, <orcid4> . ##created by orcid.
}